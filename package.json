{
  "name": "tfa-express-sample",
  "version": "1.0.0",
  "description": "An Express app to be the first application where using TFA.",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "test": "link-module-alias && nodemon server.js",
    "build": "node_modules/babel-cli/bin/babel.js ./ --source-maps --out-dir dist",
    "start": "link-module-alias && node server.js",
    "populate": "node_modules/babel-cli/bin/babel-node.js populate.js",
    "postinstall": "link-module-alias"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tele-fa/tfa-express-sample.git"
  },
  "keywords": [
    "node",
    "express",
    "tfa",
    "js",
    "mongodb",
    "api"
  ],
  "author": "Amirhossein Mohammadi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tele-fa/tfa-express-sample/issues"
  },
  "homepage": "https://github.com/tele-fa/tfa-express-sample#readme",
  "_moduleAliases": {
    "$app": "./app"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^9.0.0",
    "link-module-alias": "^1.2.0",
    "md5": "^2.3.0",
    "mongoose": "^6.4.0",
    "rayid": "^2.1.2",
    "tfa-node-sdk": "^2.6.3"
  }
